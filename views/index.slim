section#scene-seven.scene.scene-seven.cue.gray-muted-bg.quadruple data-clasp="top-hold"
  .backing.inverted
    .marker
      .content.gap-bottom-small
        h1 5. Scrub CSS Animation
      .scene-progress-bar.white-bg(
        data-character
        data-keyframe-type = "scrub-css"
        data-keyframe-start = "[0, 0.5]"
        data-keyframe-finish = "[0.5, 1]"
        data-keyframe-translate = '[ {"x" : [0,50], "y" : [0,0]}, {"x" : [50,50], "y" : [0,100]} ]'
        data-keyframe-rotate = '[ [0,0], [360,0] ]'
        data-keyframe-scale = '[ [1,1], [1,2] ]'
        data-keyframe-opacity = '[ [1,0], [0,1] ]'
        data-keyframe-blur = '[ [0,10], [10,0] ]'
      )

