            data-keyframe-type = "scrub-css"
            data-keyframe-start = "[0.2]"
            data-keyframe-finish = "0.4"
            data-keyframe-translate = '{"x" : [100,0], "y" : [0,0]}'
            data-keyframe-duration = 0.25
          )
        .column.column
          .thing.charcoal-muted-bg.pad-vert-medium.thing2
        .column
          .thing.charcoal-muted-bg.pad-vert-medium.thing3
        .column
          .thing.charcoal-muted-bg.pad-vert-medium.thing4

